{
  "escopo": "fiscal",
  "codigo": "ex-multi-entity-min",
  "descricao": "Exemplo canônico: transaction com múltiplas entidades (lista + info de configuração)",
  "tenant": 0,
  "grupo_empresarial": "00000000-0000-0000-0000-000000000000",
  "grupo_acesso": "00000000-0000-0000-0000-000000000000",
  "created_by": "versionamento@nasajon.com.br",
  "updated_by": "versionamento@nasajon.com.br",
  "json_schema": {
    "id": "fiscal_ex-multi-entity-min",
    "type": "div",
    "transaction": {
      "name": "ex-multi-entity-min",
      "entities": [
        {
          "name": "notas",
          "schema": "fiscal/NotaFiscal",
          "cardinality": "many",
          "load": true,
          "main": true,
          "loadingMessage": "Carregando notas fiscais"
        },
        {
          "name": "configuracoes",
          "schema": "fiscal/Configuracoes",
          "cardinality": "one",
          "load": true,
          "lazy": false,
          "loadingMessage": "Carregando configurações"
        }
      ]
    },
    "app_bar": {
      "pageTitle": "Notas fiscais",
      "subtitle": "Painel de testes com configurações do módulo fiscal",
      "useContextualHeader": false
    },
    "children": [
      {
        "type": "div",
        "children": [
          {
          "type": "Card",
          "props": {},
          "children": [
            {
              "type": "CardTitle",
              "children": [
                {
                  "type": "h3",
                  "children": [
                    "Configurações"
                  ]
                }
              ]
            },
            {
              "type": "CardBody",
              "children": [
                {
                  "type": "ItensList",
                  "props": {
                    "dataRef": "configuracoes.selected",
                    "includeItems": [
                      "AmbienteEmissorDocumentos",
                      "GeraCte"
                    ]
                  }
                }
              ]
            }
          ]
        },
          {
            "type": "GenericGrid",
            "props": {
              "dataRef": "notas",
              "style": {
                "maxHeight": "300px"
              },
              "resizable": true
            },
            "children": [
              {
                "type": "GridColumn",
                "props": {
                  "field": "serie",
                  "title": "Série",
                  "width": "70px"
                }
              },
              {
                "type": "GridColumn",
                "props": {
                  "field": "numero",
                  "title": "Número",
                  "width": "90px"
                }
              },
              {
                "type": "GridColumn",
                "props": {
                  "field": "data_emissao",
                  "title": "Emissão",
                  "width": "110px"
                }
              },
              {
                "type": "GridColumn",
                "props": {
                  "field": "tipo_operacao",
                  "title": "Tipo",
                  "cell": "ChipCell",
                  "width": "90px"
                }
              },
              {
                "type": "GridColumn",
                "props": {
                  "field": "situacao",
                  "title": "Situação",
                  "cell": "ChipCell",
                  "width": "110px"
                }
              },
              {
                "type": "GridColumn",
                "props": {
                  "field": "valor_total",
                  "title": "Valor total",
                  "width": "120px"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
