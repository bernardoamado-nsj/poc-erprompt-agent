                  {
                    "type": "GridColumn",
                    "props": {
                      "field": "tipo_operacao",
                      "title": "Tipo de operação",
                      "cell": "ChipCell",
                      "width": "160px"
                    }
                  },
                  {
                    "type": "GridColumn",
                    "props": {
                      "field": "situacao",
                      "title": "Situação",
                      "cell": "ChipCell",
                      "width": "140px"
                    }
                  },
