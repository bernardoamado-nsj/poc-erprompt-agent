    "cldr-numbers-full": "47.0.0",
    "date-fns": "4.1.0",
    "dotenv": "^16.6.1",
    "echarts": "5.6.0",
    "echarts-for-react": "3.0.2",
    "openai": "^6.16.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "7.7.0",
    "react-toastify": "11.0.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.16.4",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react-swc": "3.11.0",
    "chokidar-cli": "3.0.0",
    "eslint": "9.31.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "15.15.0",
    "json-server": "0.17.4",
    "typescript": "5.6.3",
    "typescript-eslint": "8.37.0",
    "vite": "6.3.5"
  }
}
