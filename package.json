{
  "name": "erprompt-agent",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "new-i18n:extract": "node --trace-deprecation ./node_modules/.bin/i18next-scanner --config .i18n-scanner.config.mjs",
    "i18n:extract": "i18next-scanner --config .i18n-scanner.config.mjs",
    "yalc_add_erprompt": "yalc add @nasajon/erprompt-lib",
    "yalc_update_erprompt": "yalc update @nasajon/erprompt-lib && rm -rf node_modules/.vite",
    "yalc_rm_erprompt": "yalc remove @nasajon/erprompt-lib",
    "yalc_add_erprompt_login": "yalc add @nasajon/erprompt-login-lib",
    "yalc_update_erprompt_login": "yalc update @nasajon/erprompt-login-lib && rm -rf node_modules/.vite",
    "yalc_rm_erprompt_login": "yalc remove @nasajon/erprompt-login-lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-server": "json-server --watch server/db.json --port 4000 --routes server/routes.json --cors",
    "serve": "node server/server.cjs",
    "gen:json": "node ./agent/call-openai-api.mjs ./agent/prompts/prompt_compilado.txt ./agent/out/result.json",
    "gen:layout": "node ./agent/call-openai-api.mjs ./agent/prompts/layout_compilado.txt ./agent/out/layout.json",
    "gen:entity": "node ./agent/call-openai-api.mjs ./agent/prompts/entity_compilado.txt ./agent/out/entity.json",
    "call:planner": "node ./agent/call-openai-planner.mjs ./agent/prompts/planner_compilado.txt ./agent/schemas/planner.schema.json ./agent/out/planner.json"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/pro-duotone-svg-icons": "6.7.2",
    "@fortawesome/pro-light-svg-icons": "6.7.2",
    "@fortawesome/pro-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@monaco-editor/react": "4.7.0",
    "@mui/material": "7.2.0",
    "@nasajon/erprompt-launcher-lib": "workspace:../../packages/erprompt-launcher-lib",
    "@nasajon/erprompt-lib": "workspace:../../packages/erprompt-lib",
    "@nasajon/erprompt-login-lib": "workspace:../../packages/erprompt-login-lib",
    "@progress/kendo-date-math": "1.5.14",
    "@progress/kendo-dateinputs-common": "0.4.5",
    "@progress/kendo-drawing": "1.21.2",
    "@progress/kendo-file-saver": "1.1.2",
    "@progress/kendo-inputs-common": "3.1.1",
    "@progress/kendo-intl": "3.1.2",
    "@progress/kendo-licensing": "1.6.0",
    "@progress/kendo-popup-common": "1.9.2",
    "@progress/kendo-react-animation": "9.5.0",
    "@progress/kendo-react-buttons": "9.5.0",
    "@progress/kendo-react-common": "9.5.0",
    "@progress/kendo-react-conversational-ui": "9.5.0",
    "@progress/kendo-react-data-tools": "9.5.0",
    "@progress/kendo-react-dateinputs": "9.5.0",
    "@progress/kendo-react-dialogs": "9.5.0",
    "@progress/kendo-react-dropdowns": "9.5.0",
    "@progress/kendo-react-editor": "9.5.0",
    "@progress/kendo-react-form": "9.5.0",
    "@progress/kendo-react-grid": "9.5.0",
    "@progress/kendo-react-indicators": "9.5.0",
    "@progress/kendo-react-inputs": "9.5.0",
    "@progress/kendo-react-intl": "9.5.0",
    "@progress/kendo-react-labels": "9.5.0",
    "@progress/kendo-react-layout": "9.5.0",
    "@progress/kendo-react-listview": "9.5.0",
    "@progress/kendo-react-notification": "9.5.0",
    "@progress/kendo-react-pdf": "9.5.0",
    "@progress/kendo-react-popup": "9.5.0",
    "@progress/kendo-react-progressbars": "9.5.0",
    "@progress/kendo-react-taskboard": "9.5.0",
    "@progress/kendo-react-treeview": "9.5.0",
    "@progress/kendo-svg-icons": "4.5.0",
    "@progress/kendo-theme-material": "10.6.0",
    "@react-keycloak/core": "3.2.0",
